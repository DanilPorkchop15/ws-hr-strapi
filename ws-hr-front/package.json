{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "format": "prettier --write --list-different \"src/**/*.{ts,tsx,js,json,yml}\"",
    "serve": "serve -s -p 80 build",
    "generate-api": "openapi -i ./src/api/restapi.yaml -o ./src/api/generated --exportSchemas true --useOptions true --useUnionTypes false --postfixServices OpenApi && npm run format-api",
    "format-api": "prettier --write --list-different \"src/api/**/*.ts\""
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/ramda": "^0.28.23",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "craco-plugin-env": "^1.0.5",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "http-proxy-middleware": "^2.0.6",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.1.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jsonous": "^8.0.0",
    "lucide-react": "^0.453.0",
    "ramda": "^0.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-redux": "^9.1.2",
    "react-router": "^6.26.2",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.1",
    "react-use": "^17.5.1",
    "reflect-metadata": "^0.2.2",
    "sass": "^1.79.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "typedi": "^0.10.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "10.*.*",
    "node": "21.*.*"
  }
}
